<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">
	<entity class="es.mdef.gesinalog_API.entidades.IncidenciaConId" access="FIELD">
		<table name="INCIDENCIAS" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<many-to-one name="instalacion" fetch="LAZY" optional="false" target-entity="es.mdef.gesinalog_API.entidades.InstalacionConId">
				<join-column name="instalacionId" />
			</many-to-one>
			<one-to-many name="valoraciones" mapped-by="incidencia" target-entity="es.mdef.gesinalog_API.entidades.Valoracion"></one-to-many>
		</attributes>
	</entity>
</entity-mappings>